<%
    function calculateVal(time) {
    var val = [];
    var half = 0;
    if (time >= 1200) {
        time = time - 1200;
        half = 1200;
    }
    timeString = time.toString(10);
    if (timeString.length == 4) {
        val.push(timeString.substring(0, 2) + '00');
        val.push(timeString.substring(2, 4));
        val.push(half);
    } else if (timeString.length == 3) {
        val.push(timeString.substring(0, 1) + '00');
        val.push(timeString.substring(1, 3));
        console.log(val);
        val.push(half);
    } else {
        val.push(0);
        val.push(time);
        val.push(half);
    }
    return val;
}

selectedFormat = function(value) {
    var val = [];
    var start = value[0];
    var end = value[1];
    
    val = calculateVal(start);
    val = val.concat(calculateVal(end));

    console.log(val);
    return val;
}
%>
